@model List<Advertisment>

@if (Model != null)
{
    foreach (var ad in Model) {
        <div style="overflow:hidden">
            <br>
            <a rel="nofollow" onclick="var id = $(this).attr('data-id');id&&$.post('@Url.Action("Click","Advertisement")', { adid: id }, $.noop);"
                target="_blank" data-id="@ad.AdID" href="@ad.AdUrl">
                <img src="@ad.ImgUrl" alt="绅士周边" />
            </a>
        </div>
    }
}